            <site name="robot10_sensor" type="sphere" size="0.2" rgba="1 0 0 0.3"/>
         </body>

        <body name="robot11" pos="-1.7 -1.4 0">
            <joint name="robot11_x" type="slide" axis="1 0 0"/>
            <joint name="robot11_y" type="slide" axis="0 1 0"/>
            <geom type="sphere" size="0.1" rgba="1 0 1 1" margin="0.05" solref="0.02 1" solimp="0.8 0.9 0.01"/>
            <site name="robot11_sensor" pos="0 0 0.1" size="0.05" type="sphere" rgba="1 1 1 0.5"/>
        </body>

        <body name="robot12" pos="-1.7 -1.4 0">
            <joint name="robot12_x" type="slide" axis="1 0 0"/>
            <joint name="robot12_y" type="slide" axis="0 1 0"/>
            <geom type="sphere" size="0.1" rgba="1 0 1 1" margin="0.05" solref="0.02 1" solimp="0.8 0.9 0.01"/>
            <site name="robot12_sensor" pos="0 0 0.1" size="0.05" type="sphere" rgba="1 1 1 0.5"/>
        </body>

        <body name="robot13" pos="-1.7 -1.4 0">
            <joint name="robot13_x" type="slide" axis="1 0 0"/>
            <joint name="robot13_y" type="slide" axis="0 1 0"/>
            <geom type="sphere" size="0.1" rgba="1 0 1 1" margin="0.05" solref="0.02 1" solimp="0.8 0.9 0.01"/>
            <site name="robot13_sensor" pos="0 0 0.1" size="0.05" type="sphere" rgba="1 1 1 0.5"/>
        </body>
    </worldbody>

    <actuator>
        <motor joint="robot1_x" ctrlrange="-9 9"/>
        <motor joint="robot1_y" ctrlrange="-9 9"/>
        <motor joint="robot2_x" ctrlrange="-9 9"/>
        <motor joint="robot2_y" ctrlrange="-9 9"/>
        <motor joint="robot3_x" ctrlrange="-9 9"/>
        <motor joint="robot3_y" ctrlrange="-9 9"/>
        <motor joint="robot4_x" ctrlrange="-9 9"/>
        <motor joint="robot4_y" ctrlrange="-9 9"/>
        <motor joint="robot5_x" ctrlrange="-9 9"/>
        <motor joint="robot5_y" ctrlrange="-9 9"/>
        <motor joint="robot6_x" ctrlrange="-9 9"/>
        <motor joint="robot6_y" ctrlrange="-9 9"/>
        <motor joint="robot7_x" ctrlrange="-9 9"/>
        <motor joint="robot7_y" ctrlrange="-9 9"/>
        <motor joint="robot8_x" ctrlrange="-9 9"/>
        <motor joint="robot8_y" ctrlrange="-9 9"/>
        <motor joint="robot9_x" ctrlrange="-9 9"/>
        <motor joint="robot9_y" ctrlrange="-9 9"/>
        <motor joint="robot10_x" ctrlrange="-9 9"/>
        <motor joint="robot10_y" ctrlrange="-9 9"/>
        <motor joint="robot11_x" ctrlrange="-9 9"/>
        <motor joint="robot11_y" ctrlrange="-9 9"/>
        <motor joint="robot12_x" ctrlrange="-9 9"/>
        <motor joint="robot12_y" ctrlrange="-9 9"/>
        <motor joint="robot13_x" ctrlrange="-9 9"/>
